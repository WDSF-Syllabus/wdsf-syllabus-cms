{
  "kind": "collectionType",
  "collectionName": "figures",
  "info": {
    "singularName": "figure",
    "pluralName": "figures",
    "displayName": "Figure",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "dance": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::dance.dance"
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "startPositions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::position.position"
    },
    "endPositions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::position.position"
    },
    "timings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::timing.timing"
    },
    "notes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::note.note",
      "inversedBy": "figures"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "startFoot": {
      "type": "enumeration",
      "enum": [
        "Right",
        "Left"
      ],
      "required": true
    },
    "endFoot": {
      "type": "enumeration",
      "enum": [
        "Right",
        "Left"
      ],
      "required": true
    },
    "startFootPlacements": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::foot-placement.foot-placement"
    },
    "endFootPlacements": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::foot-placement.foot-placement"
    }
  }
}
